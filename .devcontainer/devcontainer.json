{
  "name": "nokia.isam dev",
  "build": {
    "context": ".",
    "dockerfile": "Dockerfile"
  },
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/ansible_collections/nokia/isam,type=bind",
  "workspaceFolder": "/workspaces/ansible_collections/nokia/isam",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {},
    "ghcr.io/devcontainers-contrib/features/apt-packages:1": {
      "packages": "bash-completion"
    }
  },
  "containerEnv": {
    "ANSIBLE_COLLECTIONS_PATHS": "/workspaces/ansible_collections"
  },
  "postCreateCommand": "bash .devcontainer/postCreate.sh",
  "forwardPorts": [5678, 5679]
}
